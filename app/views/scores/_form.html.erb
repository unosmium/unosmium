<%= form_for score, as: :score, url: tournament_score_path(score.event.tournament.id, score.id) do |f| %>
  Participated?: <%= f.check_box :participated %>
  Disqualifed? <%= f.check_box :disqualified %>
  Tier <%= f.number_field :tier %> 
  Score <%= f.number_field :score, step: :any %> 
  Tiebreaker Place <%= f.number_field :tiebreaker_place %> 
  <%= f.submit %>
<% end %>
